pow(m,n) = pow(m, n-1) * m  ; n > 0
         = 1                ; n = 0


#include <stdio.h>

int pow(int m , int n) {
    if (n == 0) {
        return 1;
    } else {
        return m*pow(m, n-1);    
    }
}

int main() {
    // Write C code here
    int f = pow(2,3);
    printf("\npow (2,3) = %d", f);
    return 0;
}
